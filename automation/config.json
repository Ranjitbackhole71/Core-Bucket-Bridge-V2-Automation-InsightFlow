{
  "jobs": [
    {
      "name": "startup_sync",
      "trigger": {
        "type": "onStartup"
      },
      "actions": [
        {
          "type": "send_core_update",
          "data": {
            "module": "education",
            "data": {
              "course_id": "CS101",
              "course_name": "Introduction to Computer Science",
              "students_enrolled": 150,
              "instructor": "Dr. Smith"
            }
          }
        },
        {
          "type": "get_bucket_status"
        }
      ]
    },
    {
      "name": "periodic_finance_sync",
      "trigger": {
        "type": "intervalMinutes",
